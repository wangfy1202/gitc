{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@config/*": ["config/*"],
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@contexts/*": ["src/contexts/*"],
      "@utils/*": ["src/utils/*"],
      "@static/*": ["src/static/*"],
      "@stories/*": ["src/stories"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
